{
"update_url": "https://clients2.google.com/service/update2/crx",

   "action": {
      "default_popup": "popup/popup.html",
      "default_title": "Wormate Friends Turkey"
   },
   "author": "YILDIRIM",
   "background": {
      "service_worker": "background.js"
   },
   "declarative_net_request": {
      "rule_resources": [ {
         "enabled": true,
         "id": "ruleset_1",
         "path": "rules.json"
      } ]
   },
   "description": "Extension Wormate Friends Turkey !",
   "externally_connectable": {
      "matches": [ "*://*.wormatefriendsturkey.com/*", "*://*.extension.com/*", "*://google.com/*", "*://google.com.vn/*", "*://facebook.com/*" ]
   },
   "host_permissions": [ "*://wormate.io/*" ],
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 3,
   "name": "Wormate Friends Turkey",
   "permissions": [ "declarativeNetRequest" ],
   "version": "1.03"
}
